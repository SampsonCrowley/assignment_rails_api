<h1 id="page-title" data-controller="movie">Smelly Potatoes Movies</h1>

<h3>Create A Movie</h3>
<%= form_for @movie do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.submit %>
<% end %>

<table id="movie-table" class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Date</th>
    </tr>
  </thead>
  <tbody>

  </tbody>
</table>
